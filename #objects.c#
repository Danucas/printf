#include "holberton.h"
/**
 *get_funs - get funtioncs list
 *Return: list of functions
 */

print_t *get_funs()
{
	print_t ps[] = {
		{'c', print_char},
		{'s', print_string},
		{'d', print_dec},
		{'i', print_dec},
		{'%', print_percent},
		{'f', print},
		{'g', print},
		{'o', print},
		{'g', print},
		{'x', print},
		{'e', print}
	};
	print_t *funs = ps;

	return (funs);
}
